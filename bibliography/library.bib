keywords = {CSR,ecosystem function,functional types,leaf area,leaf thickness,leaves,resource use,tissue density},
pages = {155--162},
pmid = {21514511},
title = {{Specific leaf area and leaf dry matter content as alternative predictors of plant strategies}},
volume = {143},
year = {1999}
}
