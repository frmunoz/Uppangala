journal = {International Journal of Climatology},
keywords = {Daily rainfall distribution,Normalized rainfall curves,South-west monsoon},
number = {3},
pages = {299--311},
title = {{some aspects of daily rainfall distribution over {\{}India{\}} during the south-west monsoon season}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/joc.3370100307/abstract},
volume = {10},
year = {1990}
