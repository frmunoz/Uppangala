number = {1537},
pages = {49--60},
pmid = {20008385},
title = {{Linking biodiversity and ecosystems: towards a unifying ecological theory.}},
url = {http://apps.webofknowledge.com/full{\%}7B{\_}{\%}7Drecord.do?product=WOS{\%}7B{\&}{\%}7Dsearch{\%}7B{\_}{\%}7Dmode=CitingArticles{\%}7B{\&}{\%}7Dqid=7{\%}7B{\&}{\%}7DSID=V1TwrrLNJKUhYkGvYOi{\%}7B{\&}{\%}7Dpage=10{\%}7B{\&}{\%}7Ddoc=91{\%}7B{\&}{\%}7DcacheurlFromRightClick=no},
