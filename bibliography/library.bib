issn = {1469-8137},
journal = {New Phytologist},
keywords = {allelopathy,exotic invasive plants,functional groups,functional traits,litter decomposition,nutrients,plasticity},
number = {3},
pages = {818--828},
title = {{Invasive forbs differ functionally from native graminoids, but are similar to native forbs}},
url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1469-8137.2010.03531.x/abstract},
