editor = {Swenson, Nathan},
file = {:home/sylvain/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/C{\'{a}}rdenas et al. - 2014 - Plant traits predict inter- and intraspecific variation in susceptibility to herbivory in a hyperdiverse Neotr.pdf:pdf},
isbn = {1365-2745},
issn = {13652745},
journal = {Journal of Ecology},
keywords = {Amazonia,Ecuador,Forest dynamics plot,Janzen-Connell hypothesis,Leaf turnover,Plant-chewing-herbivore interactions,Species defence functional traits,Tree clustering,Yasun{\'{i}} National Park},
