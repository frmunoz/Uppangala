isbn = {00220477 (ISSN)},
issn = {00220477},
journal = {Journal of Ecology},
keywords = {Individual leaf area,Leaf succulence,Leaf thickness,Maximum size,Mortality,Plant development and life-history traits,Relative growth rate,Specific leaf area,Wood density},
month = {may},
number = {3},
pages = {641--650},
title = {{Linking functional traits and demographic rates in a subtropical tree community: the importance of size dependency}},
