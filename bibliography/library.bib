isbn = {0269-8463},
issn = {02698463},
journal = {Functional Ecology},
keywords = {Artificial data,Functional divergence,Functional diversity measures,Functional evenness,Functional richness,Limiting similarity,Motion model,Neutrality,Niche filtering},
number = {4},
pages = {867--876},
