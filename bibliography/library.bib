journal = {Ecological applications : a publication of the Ecological Society of America},
keywords = {Animals,Biodiversity,Computer Simulation,Demography,Likelihood Functions,Markov Chains,Models, Biological,Monte Carlo Method,Oncorhynchus,Oncorhynchus: growth {\&} development,Population Dynamics,Rivers,Rivers: chemistry},
