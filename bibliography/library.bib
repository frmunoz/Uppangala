file = {::},
isbn = {1664-462X},
issn = {1664-462X},
journal = {Frontiers in Plant Science},
keywords = {bordered pit,bordered pit, pit membrane, tracheid, vessel, vess,germany,institute for,pit membrane,systematic botany and ecology,tracheid,ulm,ulm university,vessel,vessel element,wood anatomy,zohreh karimi},
number = {March},
pages = {1--11},
pmid = {23507674},
title = {{How to quantify conduits in wood?}},
url = {http://journal.frontiersin.org/article/10.3389/fpls.2013.00056/abstract},
volume = {4},
year = {2013}
}
