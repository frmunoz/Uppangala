file = {:home/sylvain/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Le Bagousse-Pinguet et al. - 2014 - Species richness of limestone grasslands increases with trait overlap Evidence from within- and betw.pdf:pdf},
isbn = {1365-2745},
issn = {00220477},
journal = {Journal of Ecology},
keywords = {Abiotic environment,Assembly rules,Determinants of plant community diversity and stru,Functional diversity,Single- and multi-trait approaches,Species richness,Trait overlap},
month = {mar},
