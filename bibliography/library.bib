journal = {Genetics},
number = {3},
pages = {1205--1219},
pmid = {12871925},
title = {{Genetic variability at neutral markers, quantitative trait loci and trait in a subdivided population under selection}},
url = {http://www.genetics.org/content/164/3/1205.short},
volume = {164},
year = {2003}
}
