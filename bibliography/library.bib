journal = {American Journal of Botany},
keywords = {AFLP,Amplified fragment length polymorphisms,Functional diversity,Helleborus foetidus,Intraspecific variation,Leaf traits,Marker-trait association analysis,Ranunculaceae,Within-plant variation},
