isbn = {2670070000},
issn = {13652435},
journal = {Functional Ecology},
keywords = {Grime's CSR triangle,community assembly,comparative ecology,plant economics spectrum,plant functional type,survival strategy,universal adaptive strategy theory},
month = {feb},
