doi = {10.1046/j.1466-822X.2003.00042.x},
file = {:home/sylvain/Documents/Bibliography/Mendeley/Pearson, Dawson - 2003 - Predicting the impacts of climate change on the distribution of species are bioclimate envelope models useful.pdf:pdf;:home/sylvain/Documents/Bibliography/Mendeley/Pearson, Dawson - 2003 - Predicting the impacts of climate change on the distribution of species are bioclimate envelope models useful.html:html},
