doi = {10.1007/s11284-004-0003-z},
file = {:home/sylvain/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Takahashi, Seino, Kohyama - 2005 - Plastic changes of leaf mass per area and leaf nitrogen content in response to canopy openings in sap.pdf:pdf},
isbn = {0912-3814},
