issn = {1469-8137},
journal = {New Phytologist},
keywords = {CSR,Leaf thickness,ecosystem function,functional types,leaf area,leaves,resource use,tissue density},
number = {1},
pages = {155--162},
title = {{Specific leaf area and leaf dry matter content as alternative predictors of plant strategies}},
