journal = {Ecology Letters},
keywords = {Community ecology,Functional diversity,Interspecific variation,Intraspecific variability,Leaf trait,Plant functional trait,Trait-based ecology,community ecology,functional diversity,interspecific variation,intraspecific variability,leaf trait,plant functional trait,trait‐based ecology},
