isbn = {1469-8137},
issn = {14698137},
journal = {New Phytologist},
keywords = {biotic environment,competition,functional traits,local adaptation,neighbour recognition,phenotypic plasticity,selection,spatial patterns},
month = {jul},
number = {2},
pages = {455--463},
pmid = {26996338},
title = {{Plasticity in plant functional traits is shaped by variability in neighbourhood species composition}},
